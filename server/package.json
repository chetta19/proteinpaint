  },
  "files": [
    "bin.js",
    "server.js",
    "genome",
    "dataset/clinvar.*.js",
    "dataset/gdc.hg38.js",
    "utils/*.R",
    "utils/install.pp.js",
    "src/common.js",
    "server/src/serverconfig.js"
  ],
  "bugs": {
    "url": "https://github.com/stjude/pp-dist"
  }
