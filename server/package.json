{
  "name": "@stjude/proteinpaint-server",
  "version": "2.1.3",
  "description": "a genomics visualization tool for exploring a cohort's genotype and phenotype data",
  "main": "server.js",
  "bin": "bin.js",
  "imports": {
    "#shared/*": "./shared/*.js",
    "#src/*": "./src/*.js"
  },
  "scripts": {
    "dev": "webpack --config=./webpack.config.js --env NODE_ENV=development --watch --progress --color",
    "start": "nodemon server.js --watch ./server*.js* --watch genome --watch dataset",
    "pretest": "./test/pretest.js",
    "test": "tape -r '@babel/register' */test/*.spec.js",
    "build": "webpack --config=./webpack.config.js",
    "response": "nodemon modules/test/test.server.js --watch src",
    "getconf": "node -p 'JSON.stringify(require(\"./src/serverconfig.js\"),null,\"  \")'"
  },
  "author": "",
  "license": "UNLICENSED",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.9.6",
    "@babel/register": "^7.14.5",
    "babel-loader": "^8.2.2",
    "node-watch": "^0.7.1",
    "nodemon": "^1.19.1",
    "prettier": "1.18.2",
    "tape": "^5.2.2",
    "webpack": "^5.7.3",
    "webpack-cli": "^4.9.2",
    "webpack-node-externals": "^3.0.0",
    "webpack-notifier": "^1.15.0"
  },
  "dependencies": {
    "@stjude/proteinpaint-rust": "^1.0.0",
    "@babel/register": "^7.14.5",
    "better-sqlite3": "^7.5.3",
    "body-parser": "^1.15.2",
    "canvas": "2.6.0",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.5",
    "d3": "^4.12.2",
    "deep-object-diff": "^1.1.0",
    "express": "^4.17.1",
    "express-basic-auth": "^1.1.5",
    "got": "^11.5.1",
    "image-size": "^0.5.5",
    "jsonwebtoken": "^7.4.3",
    "jstat": "^1.9.3",
    "lazy": "^1.0.11",
    "node-fetch": "^2.6.1",
    "partjson": "^0.58.1",
    "tiny-async-pool": "^1.2.0",
    "webpack": "^5.73.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stjude/pp-dist"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "files": [
    "bin.js",
    "dataset/clinvar.*.js",
    "dataset/gdc*.js",
    "genome",
    "src/serverconfig.js",
    "cards/index.json",
    "server.js*",
    "shared/common.js",
    "utils/*.R",
    "utils/rust/*.*",
    "utils/rust/src"
  ],
  "bugs": {
    "url": "https://github.com/stjude/pp-dist"
  },
  "browserify": {
    "transform": [
      "browserify-css",
      [
        "babelify",
        {
          "babelrc": false,
          "presets": [
            "@babel/preset-env"
          ],
          "plugins": [
            "@babel/plugin-syntax-dynamic-import",
            "@babel/plugin-transform-runtime"
          ]
        }
      ]
    ]
  }
}
