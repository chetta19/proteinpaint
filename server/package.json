{
  "name": "@stjude/proteinpaint",
  "version": "1.8.3",
  "description": "a genomics visualization tool for exploring a cohort's genotype and phenotype data",
  "main": "dist/app.js",
  "module": "dist/app.js",
  "bin": "bin.js",
  "scripts": {
    "dev": "../node_modules/.bin/webpack --config=./webpack.config.js --env.NODE_ENV=development --watch --progress --colors",
    "start": "ln -sfn ../public && ../node_modules/.bin/nodemon server.js --watch server.js --watch genome --watch dataset --watch serverconfig.json --watch modules/test"
  },
  "author": "",
  "license": "UNLICENSED",
  "devDependencies": {
    "node-watch": "^0.7.1",
    "nodemon": "^1.19.1"
  },
  "dependencies": {
    "async": "^2.6.0",
    "better-sqlite3": "^5.4.3",
    "body-parser": "^1.15.2",
    "canvas": "^2.7.0",
    "colorbrewer": "^1.0.0",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.5",
    "d3": "^4.12.2",
    "d3-beeswarm": "0.0.4",
    "d3-sankey": "^0.4.1",
    "debounce": "^1.2.0",
    "express": "^4.17.1",
    "express-basic-auth": "^1.1.5",
    "got": "^11.5.1",
    "image-size": "^0.5.5",
    "jsonwebtoken": "^7.4.3",
    "jstat": "^1.9.3",
    "lazy": "^1.0.11",
    "loader-utils": "^0.2.17",
    "node-fetch": "^2.6.1",
    "partjson": "^0.58.1",
    "tiny-async-pool": "^1.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stjude/pp-dist"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "files": [
    "bin.js",
    "server.js",
    "genome",
    "dataset/clinvar.*.js",
    "dataset/gdc.hg38.js",
    "utils/*.R",
    "utils/install.pp.js",
    "src/common.js",
    "server/src/serverconfig.js"
  ],
  "bugs": {
    "url": "https://github.com/stjude/pp-dist"
  }
