{
  "name": "@stjude/proteinpaint",
  "version": "1.8.3",
  "description": "a genomics visualization tool for exploring a cohort's genotype and phenotype data",
  "main": "dist/app.js",
  "module": "dist/app.js",
  "bin": "bin.js",
  "scripts": {
    "dev": "webpack --config=./webpack.config.js --env.NODE_ENV=development --watch --progress --colors",
    "start": "nodemon server.js --watch ./server*.js* --watch genome --watch dataset",
    "test": "tape -r babel-register src/test/*.spec.js",
    "response": "nodemon modules/test/test.server.js --watch src"
  },
  "author": "",
  "license": "UNLICENSED",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-dynamic-import-split-require": "^2.0.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.18.0",
    "node-watch": "^0.7.1",
    "nodemon": "^1.19.1",
    "prettier": "1.18.2",
    "tape": "^4.10.1",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "^1.7.2",
    "webpack-notifier": "^1.7.0"
  },
  "dependencies": {
    "async": "^2.6.0",
    "better-sqlite3": "^5.4.3",
    "body-parser": "^1.15.2",
    "canvas": "2.6.0",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.5",
    "d3": "^4.12.2",
    "deep-object-diff": "^1.1.0",
    "express": "^4.17.1",
    "express-basic-auth": "^1.1.5",
    "got": "^11.5.1",
    "image-size": "^0.5.5",
    "jsonwebtoken": "^7.4.3",
    "jstat": "^1.9.3",
    "lazy": "^1.0.11",
    "node-fetch": "^2.6.1",
    "partjson": "^0.58.1",
    "tiny-async-pool": "^1.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stjude/pp-dist"
  },
  "publishConfig": {
