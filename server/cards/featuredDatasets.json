{
    "datasets": [
        {
            "name": "Pediatric Cancer Mutation",
            "searchbar": "gene",
            "genomeToggle": true,
            "defaultGenome": "hg19",
            "dsURLparam": "dataset=Pediatric",
            "runargs": {
                "nobox": true,
                "parselurl": true,
                "noheader": true, 
                "dataset": "Pediatric"
            }
        },
        {
            "name": "Pediatric2 GenomePaint",
            "searchbar": "all",
            "genomeToggle": true,
            "defaultGenome": "hg19",
            "dsURLparam": "block=1&mds=Pediatric2,svcnv",
            "runargs": {
                "nobox": true,
                "parselurl": true,
                "noheader": true,
                "block": true,
                "datasetqueries":[ {"dataset": "Pediatric2", "querykey": "svcnv"} ]
            }
        },
        {
            "name": "NCI GDC",
            "searchbar": "gene",
            "defaultGenome": "hg38",
            "dsURLparam": "mds3=GDC",
            "runargs": {
                "nobox": true,
                "parselurl": true,
                "noheader": true,
                "genome": "hg38",
                "tracks": [
                    {
                        "type": "mds3",
                        "dslabel": "GDC"
                    }
                ]
            }
        },
        {
            "name": "CIViC",
            "searchbar": "gene",
            "defaultGenome": "hg19",
            "dsURLparam": "dataset=CIViC",
            "runargs": {
                "nobox": true,
                "parselurl": true,
                "noheader": true,
                "dataset": "CIViC"
            }
        },
        {
            "name": "Survivorship Study",
            "link": "https://proteinpaint.stjude.org/?noheader=1&mass=%7B%22genome%22:%22hg38%22,%22dslabel%22:%20%22SJLife%22%7D"
        }
    ]
}