{
  "name": "@stjude/proteinpaint-client",
  "version": "1.10.0",
  "description": "a genomics visualization tool for exploring a cohort's genotype and phenotype data",
  "main": "dist/app.js",
  "scripts": {
    "dev": "rm -rf ../public/bin && webpack --env.NODE_ENV=development --watch --progress --colors",
    "pretest": "./test/pretest.js",
    "test": "rm -f ../public/bin/bundle.*; browserify ./test/init.js ../public/bin/proteinpaint.js src/*/test/*.spec.js -p [ split-require --out ../public/bin --public /bin ] | tape-run --static ../public",
    "browser": "rm -f ../public/bin/bundle.*; watchify src/*/test/*.spec.js -p [ split-require --out ../public/bin --public /bin ] -o ../public/bin/spec.bundle.js -v",
    "gdc": "rm -f ../public/bin/bundle.*; browserify src/wrappers/test/gdc*.spec.js -p [ split-require --out ../public/bin --public /bin ] | tape-run --static ../public",
    "rollw": "rm -rf dist && rollup -c ./client/rollup.config.js -w"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "@babel/preset-env"
          ],
          "plugins": [
            "@babel/plugin-proposal-optional-chaining",
            "@babel/transform-runtime"
          ]
        }
      ]
    ]
  },
  "author": "",
  "license": "UNLICENSED",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "babel-loader": "^8.2.2",
    "css-loader": "^0.23.1",
    "prettier": "1.18.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rollup": "^2.35.1",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-terser": "^7.0.2",
    "split-require": "^3.1.2",
    "style-loader": "^0.13.1",
    "tape": "^4.10.1",
    "tape-run": "^9.0.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "^1.7.2",
    "webpack-notifier": "^1.7.0"
  },
  "dependencies": {
    "d3": "^4.12.2",
    "d3-beeswarm": "0.0.4",
    "debounce": "^1.2.0",
    "highlight.js": "^11.0.1",
    "normalize.css": "^8.0.1",
    "partjson": "^0.58.1"
  },
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stjude/pp-dist"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "files": [
    "dist"
  ],
  "bugs": {
    "url": "https://github.com/stjude/pp-dist"
  }
}
