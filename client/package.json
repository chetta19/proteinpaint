{
  "name": "@stjude/proteinpaint-client",
  "version": "2.1.3",
  "description": "a genomics visualization tool for exploring a cohort's genotype and phenotype data",
  "main": "dist/app.js",
  "scripts": {
    "predev": "node ./test/import-specs.js name=? && rm -f ../public/bin/*proteinpaint.js",
    "dev": "webpack --env.NODE_ENV=development --watch --progress --colors",
    "pretest": "./test/pretest.js",
    "test": "node ./test/import-specs.js && npm run tape",
    "tape": "cat ../public/bin/proteinpaint.js ./test/init.js | tape-run --static ../public",
    "examples": "node ./test/import-specs.js name=_x_.examples && npm run tape",
    "gdc": "node ./test/import-specs.js name=_x_.gdc.views && npm run tape",
    "gdc-unwatched": "node ./test/import-specs.js name=_x_.gdc.views && webpack --env.NODE_ENV=development && npm run tape -- --no-sandbox",
    "rollw": "rm -rf dist && rollup -c ./rollup.config.js -w"
  },
  "author": "",
  "license": "UNLICENSED",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.14.5",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "babel-loader": "^8.2.2",
    "css-loader": "^0.23.1",
    "prettier": "1.18.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rollup": "^2.35.1",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-terser": "^7.0.2",
    "style-loader": "^0.13.1",
    "tape": "^5.2.2",
    "tape-run": "^9.0.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "^1.7.2",
    "webpack-notifier": "^1.7.0"
  },
  "dependencies": {
    "d3": "^4.12.2",
    "d3-beeswarm": "0.0.4",
    "debounce": "^1.2.0",
    "highlight.js": "~11.2.0",
    "normalize.css": "^8.0.1",
    "partjson": "^0.58.1"
  },
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stjude/pp-dist"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "files": [
    "dist"
  ],
  "bugs": {
    "url": "https://github.com/stjude/pp-dist"
  }
}
