{
  "name": "@stjude/proteinpaint",
  "version": "1.8.3",
  "description": "a genomics visualization tool for exploring a cohort's genotype and phenotype data",
  "main": "dist/app.js",
  "module": "dist/app.js",
  "bin": "bin.js",
  "scripts": {
    "dev": "rm -rf public/bin && ../node_modules/.bin/webpack --env.NODE_ENV=development --watch --progress --colors"
  },
  "browserify": {
    "transform": [
      [
        "browserify-css"
      ],
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react"
          ],
          "plugins": [
            "dynamic-import-split-require"
          ]
        }
      ]
    ]
  },
  "author": "",
  "license": "UNLICENSED",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-dynamic-import-split-require": "^2.0.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.18.0",
    "babelify": "^8.0.0",
    "browserify-css": "^0.15.0",
    "css-loader": "^0.23.1",
    "deep-object-diff": "^1.1.0",
    "electron-rebuild": "^1.8.8",
    "prettier": "1.18.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rollup": "^2.35.1",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-terser": "^7.0.2",
    "split-require": "^3.1.2",
    "style-loader": "^0.13.1",
    "tape": "^4.10.1",
    "tape-run": "^6.0.1",
    "watchify": "^3.11.1",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "^1.7.2",
    "webpack-notifier": "^1.7.0"
  },
  "dependencies": {
    "async": "^2.6.0",
    "colorbrewer": "^1.0.0",
    "d3": "^4.12.2",
    "d3-beeswarm": "0.0.4",
    "d3-sankey": "^0.4.1",
    "debounce": "^1.2.0",
    "jstat": "^1.9.3",
    "lazy": "^1.0.11",
    "loader-utils": "^0.2.17"
  },
  "peerDependencies": {
    "react": "^17.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stjude/pp-dist"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "files": [
    "dist",
    "public/index.html",
    "public/rev.txt",
    "public/bin"
  ],
  "bugs": {
    "url": "https://github.com/stjude/pp-dist"
  }
}
