{
  "name": "@stjude/proteinpaint",
  "version": "1.3.4",
  "description": "a genomics visualization tool for exploring a cohort's genotype and phenotype data",
  "main": "index.js",
  "bin": "bin.js",
  "scripts": {
    "dev": "rm -rf public/bin;webpack --config=scripts/webpack.config.dev.js  --env.NODE_ENV=development --watch --progress --colors",
    "ppdev": "node scripts/deploy.ppdev.js",
    "ppr": "node ./scripts/deploy.ppr.js",
    "ppr2": "sh ./scripts/deploy.ppr.sh http://localhost:3001",
    "ppc": "sh ./scripts/deploy.ppc.sh",
    "public-stage": "./scripts/deploy.sh public-stage",
    "public-prod": "./scripts/deploy.sh public-prod",
    "old-prod": "./scripts/deploy.sh old-prod",
    "test": "tape modules/test/*.spec.js && browserify test/init.js public/bin/proteinpaint.js src/**/test/*.spec.js | tape-run",
    "test-back": "tape modules/test/*.spec.js",
    "test-front": "browserify test/init.js public/bin/proteinpaint.js src/**/test/*.spec.js | tape-run",
    "test-browser": "watchify src/**/test/*.spec.js -o public/bin/spec.bundle.js -v",
    "test-browser-umd": "watchify -t [ babelify --presets [ env ] --plugins [ transform-es2015-modules-umd ] ] src/**/test/*.spec.js -o public/bin/spec.bundle.js -v",
    "test-response": "nodemon modules/test/test.server.js --watch modules",
    "deliver": "sh utils/deliver.sh",
    "server": "nodemon server.js --watch server.js --watch genome --watch dataset --watch serverconfig.json",
    "build-server": "webpack --config=webpack.config.server.js --mode=production",
    "sethooks": "cd .git/hooks && ln -s ../../utils/hooks/pre-commit pre-commit && chmod -R 755 ../../utils/hooks",
    "xincmd": "watchify src/toy/2/test/app.js -o public/bin/spec.bundle.js -v",
    "patch-release": "npm version patch && ./scripts/publish.sh registry",
    "minor-release": "npm version minor && ./scripts/publish.sh registry",
    "revert": "git tag -d v$(node -p \"require('./package.json').version\") && git reset --hard HEAD~1"
  },
  "prettier": {
    "semi": false,
    "useTabs": true,
    "printWidth": 120,
    "singleQuote": true
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ],
          "plugins": [
            "dynamic-import-split-require"
          ]
        }
      ]
    ]
  },
  "author": "",
  "license": "UNLICENSED",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-dynamic-import-split-require": "^2.0.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-runtime": "^6.18.0",
    "babelify": "^8.0.0",
    "css-loader": "^0.23.1",
    "deep-object-diff": "^1.1.0",
    "electron-rebuild": "^1.8.8",
    "nodemon": "^1.19.1",
    "prettier": "1.18.2",
    "split-require": "^3.1.2",
    "style-loader": "^0.13.1",
    "tape": "^4.10.1",
    "tape-run": "^6.0.1",
    "uglify-es": "^3.3.9",
    "uglify-js": "^3.4.6",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "watchify": "^3.11.1",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-node-externals": "^1.7.2",
    "webpack-notifier": "^1.7.0"
  },
  "dependencies": {
    "async": "^2.6.0",
    "babel-polyfill": "^6.9.1",
    "better-sqlite3": "^5.4.3",
    "body-parser": "^1.15.2",
    "canvas": "2.6.0",
    "colorbrewer": "^1.0.0",
    "compression": "^1.6.2",
    "d3": "^4.12.2",
    "d3-beeswarm": "0.0.4",
    "d3-sankey": "^0.4.1",
    "debounce": "^1.2.0",
    "express": "^4.17.1",
    "express-basic-auth": "^1.1.5",
    "got": "^11.5.1",
    "image-size": "^0.5.5",
    "jsonwebtoken": "^7.4.3",
    "jstat": "^1.9.3",
    "lazy": "^1.0.11",
    "loader-utils": "^0.2.17",
    "normalize.css": "^4.2.0",
    "partjson": "^0.58.1",
    "request": "^2.88.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stjude/pp-trial"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "files": [
    "server.js",
    "bin.js",
    "index.js",
    "genome",
    "dataset",
    "public/index.html",
    "public/rev.txt",
    "public/bin",
    "utils/*.R",
    "utils/install.pp.js",
    "src/common.js"
  ],
  "bugs": {
    "url": "https://github.com/stjude/pp-trial"
  }
}
