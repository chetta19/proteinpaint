{
  "name": "proteinpaint",
  "version": "1.0.0",
  "description": "a genomics visualization tool for exploring a cohort's genotype and phenotype data",
  "main": "app.js",
  "scripts": {
    "dev": "rm -rf public/bin;webpack --config=scripts/webpack.config.dev.js  --env.NODE_ENV=development --watch --progress --colors",
    "ppdev": "node scripts/deploy.ppdev.js",
    "ppr": "node ./scripts/deploy.ppr.js",
    "ppr2": "sh ./scripts/deploy.ppr.sh http://localhost:3001",
    "ppc": "sh ./scripts/deploy.ppc.sh",
    "public-stage": "./scripts/deploy.sh public-stage",
    "public-prod": "./scripts/deploy.sh public-prod",
    "old-prod": "./scripts/deploy.sh old-prod",
    "irt": "./scripts/deploy.sh internal-stage",
    "test": "tape test/**/back.*.spec.js && browserify public/bin/proteinpaint.js src/**/test/*.spec.js | tape-run",
    "test-back": "tape test/**/back.*.spec.js",
    "test-front": "browserify public/bin/proteinpaint.js src/**/test/*.spec.js | tape-run",
    "test-browser": "watchify src/**/test/*.spec.js -o public/bin/spec.bundle.js -v",
    "test-browser-umd": "watchify -t [ babelify --presets [ env ] --plugins [ transform-es2015-modules-umd ] ] src/**/test/*.spec.js -o public/bin/spec.bundle.js -v",
    "test-response": "nodemon test/test.server.js --watch test/termdb",
    "deliver": "sh utils/deliver.sh",
    "server": "nodemon server.js --watch server.js --watch genome --watch dataset --watch serverconfig.json",
    "build-server": "webpack --config=webpack.config.server.js --mode=production",
    "sethooks": "cd .git/hooks && ln -s ../../utils/hooks/pre-commit pre-commit && chmod -R 755 ../../utils/hooks",
    "xincmd": "watchify src/toy/2/test/app.js -o public/bin/spec.bundle.js -v"
  },
  "prettier": {
    "semi": false,
    "useTabs": true,
    "printWidth": 120,
    "singleQuote": true
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-runtime": "^6.18.0",
    "css-loader": "^0.23.1",
    "deep-object-diff": "^1.1.0",
    "nodemon": "^1.19.1",
    "prettier": "1.18.2",
    "style-loader": "^0.13.1",
    "tape": "^4.10.1",
    "tape-run": "^6.0.0",
    "uglify-es": "^3.3.9",
    "uglify-js": "^3.4.6",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "watchify": "^3.11.1",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-node-externals": "^1.7.2",
    "webpack-notifier": "^1.7.0"
  },
  "dependencies": {
    "async": "^2.6.0",
    "babel-polyfill": "^6.9.1",
    "babelify": "^8.0.0",
    "better-sqlite3": "^5.4.2",
    "body-parser": "^1.15.2",
    "canvas": "^2.5.0",
    "colorbrewer": "^1.0.0",
    "compression": "^1.6.2",
    "d3": "^4.12.2",
    "d3-beeswarm": "0.0.4",
    "d3-sankey": "^0.4.1",
    "debounce": "^1.2.0",
    "empty-module": "0.0.2",
    "express": "^4.17.1",
    "express-basic-auth": "^1.1.5",
    "image-size": "^0.5.5",
    "jsonwebtoken": "^7.4.3",
    "lazy": "^1.0.11",
    "loader-utils": "^0.2.17",
    "normalize.css": "^4.2.0",
    "partjson": "^0.58.1",
    "request": "^2.88.0"
  }
}
