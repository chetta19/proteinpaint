{
  "version": "1.8.3",
  "name": "@stjude/proteinpaint-full",
  "description": "a genomics visualization tool for exploring a cohort's genotype and phenotype data",
  "main": "dist/app.js",
  "module": "dist/app.js",
  "bin": "bin.js",
  "scripts": {
    "init-0": "npm install && cd server && npm install && cd ../client && npm install",
    "dev": "npm run dev --workspaces",
    "start": "nodemon server/bin.js --watch server/bin.js --watch server/server.js --watch server/genome --watch server/dataset --watch serverconfig.json --watch ./server/src/test",
    "server": "npm start",
    "ppr": "./build/deploy.sh ppr",
    "build-server": "webpack --config=build/webpack.config.server.js --env.NODE_ENV=production",
    "test": "npm test --workspaces",
    "sethooks": "cd .git/hooks && ln -s ../../utils/hooks/pre-commit pre-commit && chmod -R 755 ../../utils/hooks"
  },
  "prettier": {
    "semi": false,
    "useTabs": true,
    "printWidth": 120,
    "singleQuote": true
  },
  "author": "",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/stjude/pp-dist"
  },
  "workspaces": [
    "client",
    "server",
    "targets/gdc"
  ],
  "files": [
    "bin.js",
    "server.js",
    "dist",
    "genome",
    "dataset/clinvar.*.js",
    "dataset/gdc.hg38.js",
    "public/index.html",
    "public/rev.txt",
    "public/bin",
    "utils/*.R",
    "utils/install.pp.js",
    "src/common.js",
    "modules/serverconfig.js"
  ],
  "bugs": {
    "url": "https://github.com/stjude/pp-dist"
  }
}
