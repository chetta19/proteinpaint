{
  "name": "@stjude/proteinpaint",
  "version": "1.8.3",
  "description": "a genomics visualization tool for exploring a cohort's genotype and phenotype data",
  "main": "dist/app.js",
  "module": "dist/app.js",
  "bin": "bin.js",
  "scripts": {
    "dev": "webpack --env.NODE_ENV=development --watch --progress --colors",
    "start": "cd server && npm start",
    "dev14": "npm run dev -- --env.devtool=source-map",
    "start14": "nodemon --enable-source-maps server.js --watch server.js --watch genome --watch dataset --watch serverconfig.json --watch modules/test",
    "server": "npm start",
    "ppr": "./build/deploy.sh ppr",
    "build-server": "webpack --config=build/webpack.config.server.js --env.NODE_ENV=production",
    "test": "npm run test-back && npm run test-front",
    "deliver": "sh utils/deliver.sh",
    "sethooks": "cd .git/hooks && ln -s ../../utils/hooks/pre-commit pre-commit && chmod -R 755 ../../utils/hooks",
    "xincmd": "watchify src/toy/2/test/app.js -o public/bin/spec.bundle.js -v",
    "rolldev": "rm -rf dist && rollup -c build/rollup.config.js -w",
    "rollprod": "rm -rf dist && rollup -c build/rollup.config.js",
    "release-patch": "./build/release.sh patch",
    "release-minor": "./build/release.sh minor",
    "revert": "git tag -d v$(node -p \"require('./package.json').version\") && git reset --hard HEAD~1"
  },
  "prettier": {
    "semi": false,
    "useTabs": true,
    "printWidth": 120,
    "singleQuote": true
  },
  "author": "",
  "license": "UNLICENSED",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-dynamic-import-split-require": "^2.0.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.18.0",
    "babelify": "^8.0.0",
    "prettier": "1.18.2",
    "split-require": "^3.1.2",
    "tape": "^4.10.1",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "^1.7.2",
    "webpack-notifier": "^1.7.0"
  },
  "dependencies": {
    "d3": "^4.12.2",
    "partjson": "^0.58.1",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "^1.7.2",
    "webpack-notifier": "^1.7.0"
  },
  "peerDependencies": {
    "react": "^17.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stjude/pp-dist"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "files": [
    "bin.js",
    "server.js",
    "dist",
    "genome",
    "dataset/clinvar.*.js",
    "dataset/gdc.hg38.js",
    "public/index.html",
    "public/rev.txt",
    "public/bin",
    "utils/*.R",
    "utils/install.pp.js",
    "src/common.js",
    "modules/serverconfig.js"
  ],
  "bugs": {
    "url": "https://github.com/stjude/pp-dist"
  }
}
