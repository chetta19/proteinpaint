{
  "name": "@stjude/proteinpaint",
  "version": "1.7.0",
  "description": "a genomics visualization tool for exploring a cohort's genotype and phenotype data",
  "main": "dist/app.js",
  "module": "dist/app.js",
  "bin": "bin.js",
  "scripts": {
    "dev": "rm -rf public/bin;webpack --config=build/webpack.config.dev.js --env.NODE_ENV=development --watch --progress --colors",
    "start": "nodemon server.js --watch server.js --watch genome --watch dataset --watch serverconfig.json --watch modules/test",
    "dev12": "npm run dev -- --env.devtool=source-map",
    "start12": "nodemon --enable-source-maps server.js --watch server.js --watch genome --watch dataset --watch serverconfig.json --watch modules/test",
    "server": "npm start",
    "ppr": "./build/deploy.sh ppr",
    "build-server": "webpack --config=build/webpack.config.server.js --env.NODE_ENV=production",
    "test": "npm run test-back && npm run test-front",
    "test-back": "tape -r babel-register modules/test/*.spec.js",
    "test-front": "browserify test/init.js public/bin/proteinpaint.js src/*/test/*.spec.js -p [ split-require --out ./public/bin --public /bin ] | tape-run --static public",
    "test-browser": "rm -f public/bin/bundle.*; watchify src/*/test/*.spec.js -p [ split-require --out ./public/bin --public /bin ] -o public/bin/spec.bundle.js -v",
    "test-response": "nodemon modules/test/test.server.js --watch modules",
    "test-gdc": "browserify test/init.js public/bin/proteinpaint.js src/wrappers/test/gdc*.spec.js -p [ split-require --out ./public/bin --public /bin ] | tape-run --static public",
    "deliver": "sh utils/deliver.sh",
    "sethooks": "cd .git/hooks && ln -s ../../utils/hooks/pre-commit pre-commit && chmod -R 755 ../../utils/hooks",
    "xincmd": "watchify src/toy/2/test/app.js -o public/bin/spec.bundle.js -v",
    "rolldev": "rm -rf dist && rollup -c build/rollup.config.js -w",
    "rollprod": "rm -rf dist && rollup -c build/rollup.config.js",
    "release-patch": "./build/release.sh patch",
    "release-minor": "./build/release.sh minor",
    "revert": "git tag -d v$(node -p \"require('./package.json').version\") && git reset --hard HEAD~1"
  },
  "prettier": {
    "semi": false,
    "useTabs": true,
    "printWidth": 120,
    "singleQuote": true
  },
  "browserify": {
    "transform": [
      [
        "browserify-css"
      ],
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react"
          ],
          "plugins": [
            "dynamic-import-split-require"
          ]
        }
      ]
    ]
  },
  "author": "",
  "license": "UNLICENSED",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-dynamic-import-split-require": "^2.0.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.18.0",
    "babelify": "^8.0.0",
    "browserify-css": "^0.15.0",
    "css-loader": "^0.23.1",
    "deep-object-diff": "^1.1.0",
    "electron-rebuild": "^1.8.8",
    "node-watch": "^0.7.1",
    "nodemon": "^1.19.1",
    "prettier": "1.18.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rollup": "^2.35.1",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-terser": "^7.0.2",
    "split-require": "^3.1.2",
    "style-loader": "^0.13.1",
    "tape": "^4.10.1",
    "tape-run": "^6.0.1",
    "uglify-es": "^3.3.9",
    "uglify-js": "^3.4.6",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "watchify": "^3.11.1",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "^1.7.2",
    "webpack-notifier": "^1.7.0"
  },
  "dependencies": {
    "async": "^2.6.0",
    "better-sqlite3": "^5.4.3",
    "body-parser": "^1.15.2",
    "canvas": "^2.6.0",
    "colorbrewer": "^1.0.0",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.5",
    "d3": "^4.12.2",
    "d3-beeswarm": "0.0.4",
    "d3-sankey": "^0.4.1",
    "debounce": "^1.2.0",
    "express": "^4.17.1",
    "express-basic-auth": "^1.1.5",
    "got": "^11.5.1",
    "image-size": "^0.5.5",
    "jsonwebtoken": "^7.4.3",
    "jstat": "^1.9.3",
    "lazy": "^1.0.11",
    "loader-utils": "^0.2.17",
    "node-fetch": "^2.6.1",
    "normalize.css": "^4.2.0",
    "partjson": "^0.58.1",
    "tiny-async-pool": "^1.2.0"
  },
  "peerDependencies": {
    "react": "^17.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stjude/pp-dist"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "files": [
    "bin.js",
    "server.js",
    "dist",
    "genome",
    "dataset/clinvar.*.js",
    "dataset/gdc.hg38.js",
    "public/index.html",
    "public/rev.txt",
    "public/bin",
    "utils/*.R",
    "utils/install.pp.js",
    "src/common.js",
    "modules/serverconfig.js"
  ],
  "bugs": {
    "url": "https://github.com/stjude/pp-dist"
  }
}
