{
	"name": "@stjude/proteinpaint-pp-irt",
	"version": "2.5.0",
	"private": true,
	"scripts": {
		"postinstall": "proteinpaint-front https://pp-int-test.stjude.org",
		"postupdate": "proteinpaint-front https://pp-int-test.stjude.org",
		"start": "proteinpaint-server",
		"static": "rsync -Lr --verbose --compress --delete -e ssh --exclude-from '../exclude-list.txt' --stats ./ pp-irt:/opt/app/pp/active/"
	},
	"dependencies": {
		"@stjude/proteinpaint-front": "2.5.0",
		"@stjude/proteinpaint-server": "2.5.0"
	},
	"files": [
		"public/*",
		"dataset/*",
		"genome/*",
		"server.js",
		"rev.txt"
	]
}
